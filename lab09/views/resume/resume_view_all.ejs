<!DOCTYPE html>
<html>
<head>
    <title>Resume - View All</title>
</head>
<body>

<h1>Resumes</h1>
<a href="/resume/add"> Add New Resume</a> <br />
<table border="1">
    <tr>
        <th>Resume ID</th>
        <th>Account ID</th>
        <th>Resume Name</th>
        <th>Actions</th>
    </tr>
    <% for(var i = 0; i < resumes.length; i++) { %>
    <tr>
        <td><a href="/resume/?resume_id=<%= resumes[i].resume_id %>">
                <%= resumes[i].resume_id %>
            </a></td>
        <td><a href="/resume/?resume_id=<%= resumes[i].account_id %>">
                <%= resumes[i].account_id %>
            </a></td>
        <td><a href="/resume/?resume_id=<%= resumes[i].resume_name %>">
                <%= resumes[i].resume_name %>
            </a></td>

        <td><a href="/resume/edit/?resume_id=<%= resumes[i].resume_id %>">
                Edit
            </a>
        </td>
        <td><a href="/resume/delete/?resume_id=<%= resumes[i].resume_id %>">
                Delete
            </a></td>

    </tr>
    <% } %>
</table>

</body>
</html>